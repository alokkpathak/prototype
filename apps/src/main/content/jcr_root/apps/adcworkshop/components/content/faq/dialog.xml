<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <categoryName
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Type of Catagory"
                        name="./categoryname"
                        xtype="textfield"/>
                    <faqPageLink
                        jcr:primaryType="cq:Widget"
                        fieldLabel="faq Page Path"
                        name="./faqpath"
                        xtype="pathfield"/>
                    <faqerror
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Label for Error content"
                        name="./ferror"
                        xtype="textfield"/>
					<faqrunmodepage
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Label for Redirecting page in publish"
                        name="./redirectpath"
                        xtype="pathfield"/>	
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
