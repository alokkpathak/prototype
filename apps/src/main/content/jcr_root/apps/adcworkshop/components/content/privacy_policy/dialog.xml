<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="privacy_policy"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Text">
                <items jcr:primaryType="cq:WidgetCollection">
                    <headline
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Top- Headlin"
                        name="./richtext2"
                        xtype="richtext">
                        <rtePlugins
                            jcr:primaryType="nt:unstructured"
                            features="*">
                            <paraformat
                                jcr:primaryType="nt:unstructured"
                                features="*">
                                <formats jcr:primaryType="cq:WidgetCollection">
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        description="Heading1"
                                        tag="h1"/>
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        description="Heading2"
                                        tag="h2"/>
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        description="Heading3"
                                        tag="h3"/>
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        description="Heading4"
                                        tag="h4"/>
                                    <p
                                        jcr:primaryType="nt:unstructured"
                                        description="paragraph"
                                        tag="p"/>
                                </formats>
                            </paraformat>
                            <styles
                                jcr:primaryType="nt:unstructured"
                                features="*">
                                <styles jcr:primaryType="cq:WidgetCollection">
                                    <content
                                        jcr:primaryType="nt:unstructured"
                                        cssName="well well-sm"
                                        text="Content"/>
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        cssName="margin-about"
                                        text="heading"/>
                                    <table
                                        jcr:primaryType="nt:unstructured"
                                        cssName="table table-bordered"
                                        text="table"/>
                                </styles>
                            </styles>
                            <misctools
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <image
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <edit jcr:primaryType="nt:unstructured"/>
                            <format
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <subsuperscript
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                            <table
                                jcr:primaryType="nt:unstructured"
                                features="*"/>
                        </rtePlugins>
                    </headline>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
