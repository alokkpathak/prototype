<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Orders Links"
    xtype="tabpanel">
    <items jcr:primaryType="cq:WidgetCollection">
        <orderLinks
            jcr:primaryType="cq:Widget"
            title="Order Links"
            xtype="panel">
            <items jcr:primaryType="cq:WidgetCollection">
                <multilinks
                    jcr:primaryType="cq:Widget"
                    baseName="orderlinks_"
                    fieldLabel="Order Links"
                    name="./orderlinks"
                    xtype="mtmulticompositefield">
                    <fieldConfigs jcr:primaryType="cq:WidgetCollection">
                        <linkTitle
                            jcr:primaryType="cq:Widget"
                            allowBlank="false"
                            fieldLabel="Title"
                            name="orderLinkName"
                            width="300"
                            xtype="textfield"/>
                        <linkPath
                            jcr:primaryType="cq:Widget"
                            allowBlank="false"
                            fieldLabel="Link"
                            name="orderLink"
                            width="300"
                            xtype="textfield"/>
                    </fieldConfigs>
                </multilinks>
                <shipmentinfo
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Label for Shipment Info"
                    name="./shipment"/>
            </items>
        </orderLinks>
    </items>
</jcr:root>
