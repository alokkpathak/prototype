<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="header"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <headerbar
                jcr:primaryType="cq:Panel"
                title="Edit Panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <leftlogo
                        jcr:primaryType="cq:Widget"
						fieldDescription="Upload only .jpg or .png images of 1MB or less with 304x100 px Resolution"
                        fieldLabel="Left logo Image"
                        name="./imageleft"
                        width="300"
                        xtype="pathfield"/>
                    <leftlogoMobile
                        jcr:primaryType="cq:Widget"
						fieldDescription="Upload only .jpg or .png images of 1MB or less with 304x100 px Resolution"
                        fieldLabel="Left logo Image for mobile"
                        name="./imageleftMobile"
                        width="300"
                        xtype="pathfield"/>
                    <FSL_logoLink
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Select link"
                        name="./fsllink"
                        xtype="pathfield"/>
                    <rightlogo
                        jcr:primaryType="cq:Widget"
						fieldDescription="Upload only .jpg or .png images of 1MB or less with 150 x 157 px Resolution"
                        fieldLabel="Right logo Image"
                        name="./imageright"
                        width="300"
                        xtype="pathfield"/>
                    <rightlogoMobile
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Right logo Image for mobile"
                        name="./imagerightMobile"
                        width="300"
                        xtype="pathfield"/>
                    <vorwerk_logoLink
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Select link"
                        name="./vorwerklink"
                        xtype="pathfield"/>
                    <myaccountLogo
                        jcr:primaryType="cq:Widget"
						fieldDescription="Upload only .jpg or .png images of 1MB or less with 30 x 32 Px Resolution"
                        fieldLabel="My account Image"
                        name="./cartLogo"
                        width="300"
                        xtype="pathfield"/>
                    <myaccountLogoMobile
                        jcr:primaryType="cq:Widget"
						fieldDescription="Upload only .jpg or .png images of 1MB or less with 30 x 32 Px Resolution"
                        fieldLabel="My account Image for mobile"
                        name="./cartLogoMobile"
                        width="300"
                        xtype="pathfield"/>
					<altimagemyaccountLogo
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Enter the text for My account Image alt"
                        name="./imagealtMyaccount"
						width="300"
                        xtype="textfield"/>			
                    <myaccountLogoSignedin
                        jcr:primaryType="cq:Widget"
                        fieldLabel="My account Signedin Image"
                        name="./myaccountLogoSignedin"
                        width="300"
                        xtype="pathfield"/>
                    <myAccount_Link
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Select link"
                        name="./accountlink"
                        xtype="pathfield"/>
                    <myBasket
                        jcr:primaryType="cq:Widget"
						fieldDescription="Upload only .jpg or .png images of 1MB or less with 30 x 32 Px Resolution"
                        fieldLabel="My Basket Image"
                        name="./myBasket"
                        width="300"
                        xtype="pathfield"/>
                    <myBasketMobile
                        jcr:primaryType="cq:Widget"
						fieldDescription="Upload only .jpg or .png images of 1MB or less with 30 x 32 Px Resolution"
                        fieldLabel="My Basket Image for mobile"
                        name="./myBasketMobile"
                        width="300"
                        xtype="pathfield"/>
					<altimagemyBasket
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Enter the text for My Basket Image alt"
                        name="./imagealtMyBasket"
						width="300"
                        xtype="textfield"/>		
                    <basket_Link
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Select link"
                        name="./basketlink"
                        xtype="pathfield"/>
                    <Text1
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Enter my account heading"
                        maxLength="18"
                        name="./text1"
                        xtype="textfield"/>
                    <myAccountLink
                        jcr:primaryType="cq:Widget"
                        fieldLabel="My Account Overview Path:"
                        name="./myAccountLink"
                        xtype="pathfield"/>
                    <Text2
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Enter cart text"
                        maxLength="18"
                        name="./text2"
                        xtype="textfield"/>
                    <searchResultsLink
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Search Results Page"
                        name="./searchResultsLink"
                        xtype="pathfield"/>
                    <logoutlabel
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Logout Label"
                        name="./logoutlabel"
                        xtype="textfield"/>
                    <logoutUrl
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Logout URL"
                        name="./logoutUrl"
                        xtype="pathfield"/>
                    <menuButton
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Menu Button Path Label"
                        name="./menubutton"
                        width="300"
                        xtype="pathfield"/>
                    <menuButtonMobile
                        jcr:primaryType="cq:Widget"
						fieldDescription="Upload only .jpg or .png images of 1MB or less with 32 x 32 Px Resolution"
                        fieldLabel="Menu Button Path Label For Mobile"
                        name="./menubuttonMobile"
                        width="300"
                        xtype="pathfield"/>
                    <menuArrow
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Menu  Arrow Path Label"
                        name="./menuarrow"
                        width="300"
                        xtype="pathfield"/>
                    <menuArrowMobile
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Menu Arrow Path Label for mobile"
                        name="./menuarrowMobile"
                        width="300"
                        xtype="pathfield"/>
                    <searchsymbol
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Search symbol Path Label"
                        name="./search"
                        width="300"
                        xtype="pathfield"/>
                    <searchsymbolMobile
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Search symbol Path Label for mobile"
                        name="./searchMobile"
                        width="300"
                        xtype="pathfield"/>
                </items>
            </headerbar>
            <navBar
                jcr:primaryType="cq:Panel"
                title="Navbar edit panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <navlist
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Add your title"
                        name="./subNavTitles"
                        xtype="multifield"/>
					<displayBasketLink
						jcr:primaryType="cq:Widget"
						value="yes"
						defaultValue="yes"
						fieldLabel="Is Basket Link displayable?"
						name="./displayBasketLink"
						type="radio"
						xtype="selection">
						<options jcr:primaryType="cq:WidgetCollection">
							<option1
								jcr:primaryType="nt:unstructured"
								text="Yes"
								value="yes"/>
							<option2
								jcr:primaryType="nt:unstructured"
								text="No"
								value="no"/>
						</options>
					</displayBasketLink>
					<displayMyAccountLink
						jcr:primaryType="cq:Widget"
						value="yes"
						defaultValue="yes"
						fieldLabel="Is My Account Link displayable?"
						name="./displayMyAccountLink"
						type="radio"
						xtype="selection">
						<options jcr:primaryType="cq:WidgetCollection">
							<option1
								jcr:primaryType="nt:unstructured"
								text="Yes"
								value="yes"/>
							<option2
								jcr:primaryType="nt:unstructured"
								text="No"
								value="no"/>
						</options>
					</displayMyAccountLink>
                    <selectSearchBox
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Include search box  "
                        name="./select"
                        type="checkbox"
                        xtype="selection"/>
                    <searchResURL
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Enter the link to Redirect eg: /content/adc/fsl/countries/gb/en/searchresults"
                        fieldLabel="Search Results URL"
                        name="./searchResURL"
                        width="300"
                        xtype="pathfield"/>
                    <errorPageURL
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Enter the link to Redirect eg: /content/adc/fsl/countries/gb/en/utilities/errors/500"
                        fieldLabel="Error page path"
                        name="./errorPageURL"
                        width="300"
                        xtype="pathfield"/>
                    <unauthorizedPageURL
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Enter the link to Redirect eg: /content/adc/fsl/countries/gb/en/utilities/errors/unauthorized"
                        fieldLabel="Unauthorized access page path"
                        name="./unauthorizedPageURL"
                        width="300"
                        xtype="pathfield"/>
                    <transactiontimeout
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Enter the link for Transaction timeout Page"
                        fieldLabel="Transaction time out URL"
                        name="./transactiontimeoutUrl"
                        width="300"
                        xtype="pathfield"/>
                </items>
            </navBar>
        </items>
    </items>
    <listeners
        jcr:primaryType="nt:unstructured"
        beforesubmit="function(dialog) {&#xa;var submit=true;&#xa;var multi = dialog.findByType('multifield')[0];&#xa;var textfields = multi.findByType('textfield');&#xa;&#x9; for(var i=0; i &lt; textfields.length; i++) {&#xa;&#x9;&#x9; console.log(&quot;each value>>>>&quot;+textfields[i].getValue().trim().length);&#xa;&#x9;&#x9; if(textfields[i].getValue().trim().length>18){&#xa;&#x9;&#x9;&#x9;CQ.Ext.Msg.show({title: 'Enter value',&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; msg: 'Maximum character limit reached, Verify all fields and try again.',&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; icon:CQ.Ext.MessageBox.WARNING,&#xa;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; buttons: CQ.Ext.Msg.OK} ); &#xa;&#x9;&#x9;&#x9;submit=false;&#xa;&#x9;&#x9; }&#xa;&#x9; }&#xa;&#x9; return submit;&#xa;}"/>
</jcr:root>
